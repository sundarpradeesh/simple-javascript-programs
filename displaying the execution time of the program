<html>
	<head>
		<title>code-time</title>
	</head>
	<script>
	function checkprime()
	{
	var n=prompt("enter a no");
	var c=0;
	for(i=2;i*i<=n;i++)
	{
	if(n%i==0)
	{window.alert("not prime");
	break;}
	
	else
	window.alert("prime");
	}
	}
	var start=Date.now();
	checkprime();
	var end=Date.now();
	var aa=end-start;
	window.alert("time : "+aa);
	</script>
	</body>
</html>
